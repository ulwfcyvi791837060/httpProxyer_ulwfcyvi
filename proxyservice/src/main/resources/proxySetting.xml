<?xml version="1.0" encoding="UTF-8"?>
<Proxy>
	<defaultSettings>
		<port>8333</port>
		<basePath>/</basePath>
	</defaultSettings>

	<ProxyServices>
		<ProxyService>
			<proxyUrl>/*</proxyUrl>
			<targetUrl>https://webapp.91cpct.com</targetUrl>
		</ProxyService>

		<ProxyService>
			<proxyUrl>/baidu/*</proxyUrl>
			<targetUrl>https://www.baidu.com</targetUrl>
		</ProxyService>

		<ProxyService>
			<proxyUrl>/test/*</proxyUrl>
			<targetUrl>http://localhost:9090</targetUrl>
		</ProxyService>

	</ProxyServices>

</Proxy>
